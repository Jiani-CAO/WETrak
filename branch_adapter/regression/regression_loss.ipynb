   "metadata": {},
   "outputs": [],
   "source": [
    "def mse_loss(y_pred, y_true):\n",
    "    \n",
    "    loss_mse = torch.mean((y_pred - y_true) ** 2)\n",
    "    \n",
    "    return loss_mse"
   ]
  }
